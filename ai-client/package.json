{
  "name": "@jumoo/translate-ai",
  "version": "16.0.0-rc4.3",
  "licence": "Custom",
  "description": "Jumoo ai connector",
  "type": "module",
  "author": {
    "name": "Jumoo",
    "email": "info@jumoo.co.uk",
    "url": "https://jumoo.co.uk"
  },
  "exports": {
    ".": "./dist/index.js",
    "./api": "./dist/api/index.js"
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "client:build": "tsc --project ./tsconfig.dist.json",
    "client:pack": "npm pack --pack-destination ./pack",
    "client:publish": "npm publish",
    "client:make": "npm run client:build && npm run client:pack && npm run client:publish",
    "client:link": "npm link",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "generate": "rm-rs -r -f ./src/api && openapi-ts",
    "format:fix": "npm run format -- --write",
    "format": "prettier \"src/**/*.ts\" --check"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.10.0",
    "@hey-api/openapi-ts": "^0.71.1",
    "@jumoo/translate": "^16.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@umbraco-cms/backoffice": "^16.0.0",
    "lit": "^3.1.3",
    "prettier": "^3.3.3",
    "rm-rs": "^0.2.3",
    "typescript": "^5.6.2",
    "vite": "^5.2.12",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
